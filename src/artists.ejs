<% const {menubar} = require('./menubar.js') %>

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Исполнители</title>
    <link rel="icon" type="image/x-icon" href="images/favicon.svg" />
    <analytics></analytics>
  </head>
  <body>
